{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
    "js": "babel-node ./src/server/index.js"
  },
  "events": {
    "restart":
      "echo '> server changed, restarting...'"
  },
  "watch": ["src/server", "src/data"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js"
}
